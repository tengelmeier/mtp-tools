Table _MTPObjectPropcodeReservedTable
{
    switch
    {
        case ((value>=MTP_OBJECTPROPCODE_VENDOREXTENSION_FIRST)&& (value<=MTP_OBJECTPROPCODE_VENDOREXTENSION_LAST)): "VendorExtension";
        default: "Unknown";
    }
}

